    <Dialog id="addItemBox" title="Add an item by article number">
        <HBox justifyContent="Center" class="sapUiSmallMargin">
            <List id="listItems" mode="Delete" items="{path: 'tableData>/articles'}"></List>
        </HBox>
        <beginButton>
            <Button text="Close" press=".onCloseAddItem" class="sapUiNoMarginBottom" type="Emphasized"></Button>
        </beginButton>
    </Dialog>
